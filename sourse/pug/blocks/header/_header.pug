mixin header()
	// start header
	+b.HEADER.header--js#header&attributes(attributes)
		.container
			.row.justify-content-between.align-items-center
				.col-auto
					+e.img-box
						img(src="img/svg/ekontradelogo.svg" alt="" loading="lazy")
				.col-auto
					+e.call-stroke
						.row.align-items-center
							.col-auto
								a(href="tel:+380969668228") +380 (96) 966 82 28
							.col-auto
								.btn.btn-primary Замовити Дзвінок
	// end header